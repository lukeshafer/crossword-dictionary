{
  // "extends": "@tsconfig/node18/tsconfig.json",
  "include": [
    "./src/*"
  ],
  "compilerOptions": {
    "types": [
      "@aws-lite/dynamodb-types"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "ES2022"
  }
}
